<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>panel.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>panel</object-name>

    <!-- Objets transparents -->
  </animation>

  <!-- ASI -->
  <model>
    <path>Aircraft/Bleriot-SPAD-S.510/Models/Interior/Panel/Instruments/airspeed/airspeed.xml</path>
    <offsets>
      <x-m>  0.001 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.008 </z-m>
    </offsets>
  </model>

  <!-- COMPASS -->
  <model>
    <path>Aircraft/Bleriot-SPAD-S.510/Models/Interior/Panel/Instruments/compass/compass.xml</path>
    <offsets>
      <x-m> 0.005 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 0.050 </z-m>
    </offsets>
  </model>

  <!-- VSI -->
  <model>
    <path>Aircraft/Bleriot-SPAD-S.510/Models/Interior/Panel/Instruments/vsi/vsi.xml</path>
    <offsets>
      <x-m>  0.004 </x-m>
      <y-m>  0.062 </y-m>
      <z-m> -0.036 </z-m>
    </offsets>
  </model>

  <!-- ALT -->
  <model>
    <path>Aircraft/Bleriot-SPAD-S.510/Models/Interior/Panel/Instruments/alt/alt.xml</path>
    <offsets>
      <x-m>  0.001 </x-m>
      <y-m> -0.060 </y-m>
      <z-m>  0.050 </z-m>
    </offsets>
  </model>

  <!-- CLOCK -->
  <model>
    <path>Aircraft/Bleriot-SPAD-S.510/Models/Interior/Panel/Instruments/clock/clock.xml</path>
    <offsets>
      <x-m>  0.001 </x-m>
      <y-m> -0.122 </y-m>
      <z-m> -0.007 </z-m>
    </offsets>
  </model>

</PropertyList>
